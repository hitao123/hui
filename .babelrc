{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "commonjs": {
      "presets": [
        "stage-2",
        [
          "env",
          {
            "modules": "commonjs",
            "loose": true
          }
        ]
      ]
    },
    "test": {
      "presets": ["env", "stage-2"]
    }
  }
}
