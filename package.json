{
  "name": "tui-ui",
  "version": "1.0.3",
  "description": "A Vue.js ui tiny mobile framework",
  "author": "hitao1234@gmail.com",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/less/index.less",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "packages",
    "types"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "npm run build:entry && webpack-dev-server --inline --progress --config build/webpack.dev.js",
    "lint": "eslint --ext .js,.vue src test/unit && stylelint \"packages/*.less\"",
    "build:entry": "node build/build-entry",
    "build:hui": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.js",
    "build:components": "node build/build-components.js --color",
    "build:style": "gulp --gulpfile packages/less/gulpfile.js --color --silent",
    "build:style-entry": "node build/build-style-entry.js",
    "build:lib": "node build/build-lib.js",
    "build:site": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.js  && gh-pages -d docs/dist",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit -- -u",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i ./docs/markdown/changelog.README.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hitao123/hui.git"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue-lazyload": "1.2.6",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17"
  },
  "devDependencies": {
    "@vue/server-test-utils": "^1.0.0-beta.24",
    "@vue/test-utils": "^1.0.0-beta.24",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "commitizen": "^2.10.1",
    "conventional-changelog": "^2.0.1",
    "conventional-changelog-cli": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^2.1.0",
    "dependency-tree": "^6.3.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fast-glob": "^2.2.3",
    "fast-vue-md-loader": "^1.0.3",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^2.0.1",
    "ghooks": "^2.0.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.0.0",
    "gulp-less": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "hui-doc": "^1.1.6",
    "inject-loader": "^3.0.0",
    "jest": "^23.5.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.8",
    "signale": "^1.3.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.5.8",
    "validate-commit-msg": "^2.14.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.3.0",
    "vue-sfc-compiler2": "1.0.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": "*",
      "warnOnFail": false,
      "maxSubjectLength": 200,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入message信息!",
      "helpMessage": "Commit message 格式错误, 请按照\"<type>(<scope>): <subject>\" !方式提交记录 或者使用 npm run commit 辅助提交"
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
